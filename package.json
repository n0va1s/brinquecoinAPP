{
    "name": "brinquecoin",
    "version": "1.0.0",
    "description": "Um aplicativo para ajudar pais, mães e responsáveis na arte de criar filhos",
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "start": "http-server -c-1"
    },
    "devDependencies": {
        "axios": "^0.21",
        "cross-env": "^7.0.3",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.32.11",
        "sass-loader": "^11.0.1",
        "vue-template-compiler": "^2.6.12",
        "workbox-build": "^4.3.1"
    },
    "keywords": [
        "Brinque Coin",
        "Quadro de Tarefas",
        "Quadro de Mesada",
        "Aplicativo para Criança",
        "Criação de Filhhos"
    ],
    "author": {
        "name": "João Paulo Novais",
